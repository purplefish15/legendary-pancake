/*Program to print number triangle :
1
2 2
3 3 3
4 4 4 4 .... */
#include<stdio.h>
void main(){
    int n;
    printf("Enter number of rows: ");
    scanf("%d",&n);
    int a=1;
    for(int i=1;i<=n;i++){
        for(int j=1;j<=i;j++){
            printf("%d ",a);
        }
        a++;
        printf("\n");
    }
}
