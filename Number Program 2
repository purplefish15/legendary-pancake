/*Program to check whether the given number is prime or not.*/
#include<stdio.h>
#include<math.h>
void main(){
    int n,flag=0;
    printf("Enter a number: ");
    scanf("%d",&n);
    for(int i=2;i<=n/2;i++){
        if(n%i==0){
            flag++;
        }
    }
    if(flag){
    printf("The number %d is not prime.",n);
    }
    else{
    printf("The number %d is prime.",n);
    }
}
