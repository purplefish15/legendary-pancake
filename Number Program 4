/*Program to print the Fibonacci Series. */
#include<stdio.h>
void main(){
    int n, f1=0,f2=1;
    int i=1;
    printf("Enter the number of terms to be displayed: ");
    scanf("%d",&n);
    while(i<=n){
        int f=f1+f2;
        f1=f2;
        f2=f;
        printf("%d ",f1);
        i++;
    }
}
