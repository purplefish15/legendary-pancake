/*Write a program to print all the Pythagorean triplets between 1 to 60 avoid repetition (3^2 + 4^2
= 5^2 is similar to 4^2 + 3^2 = 5^2 and should be avoided)*/


#include <math.h>
#include <stdio.h>

int main() {
    int j,i,k,l[60];
    int fish;
    for(i=1;i<=60;i++)
    {
        for(j=i;j<=60;j++)
        {
            fish=i*i+j*j;
            k=sqrt(fish);
            if(k*k==fish){
                printf("\n %d %d %d",i,j,k);
                l[j]=j;
            }
        }
        
    }
    return 0;
}
