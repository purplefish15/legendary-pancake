/*Write a program to generate the number triangle: 
1
2 3
4 5 6
7 8 9 0*/
#include<stdio.h>
#include<math.h>
void main(){
    int i,j,k=1;
    for(i=1;i<=4;i++){
        for(j=1;j<=i;j++){
            if(k==10)
            continue;
            printf("%d ",k);
            k++;
        }
        if(k==10)
        printf("0 ");
        printf("\n");
    }
}
